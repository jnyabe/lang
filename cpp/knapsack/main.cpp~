#include <vector>
#include <random>
#include <iostream>

struct Item
{
	int cost; 
	int weight;
};


void init(std::vector& items)
{
	std::random_device rnd;
	std::mt19937 mt(rnd());
	for(auto i: items)
	{
		i.cost   = 0;
		i.weight = 0;
	}
}

int main(int argc, const char* argv[])
{
	int n=10; // num of items
	std::vector items(n);
	
	return 0;
}
